# Conduit Server — Environment Variables
# Copy to .env and fill in your keys

# Ollama Cloud
OLLAMA_API_KEY=

# NVIDIA NIM
NIM_API_KEY=

# Google Cloud — Vertex AI (uses GCP credits)
GCP_PROJECT=your-project-id
# Auth: run `gcloud auth application-default login`
# Or: set GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Anthropic Opus (budget-capped)
ANTHROPIC_API_KEY=

# Outlook (Microsoft Graph — personal account)
OUTLOOK_CLIENT_ID=

# Google Calendar (run scripts/gcal_auth.py to get refresh token)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REFRESH_TOKEN=
GOOGLE_CALENDAR_ID=primary
